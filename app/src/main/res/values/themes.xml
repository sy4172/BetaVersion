<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BetaVersion" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#8f5a1f</item>
        <item name="colorPrimaryVariant">#fe991d</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#fe991d</item>
        <item name="colorSecondaryVariant">#737373</item>
        <item name="colorOnSecondary">#231F01</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.BetaVersion.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.BetaVersion.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.BetaVersion.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar" />
</resources>